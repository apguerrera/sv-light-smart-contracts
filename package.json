  "repository": "git@github.com:secure-vote/sv-light-smart-contracts.git",
  "author": "Max Kaye <max@secure.vote>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "async-parallel": "^1.2.3",
    "colors": "^1.2.1",
    "eth-ens-namehash": "^2.0.8",
    "ethereumjs-testrpc": "^6.0.3",
    "ethereumjs-util": "^5.1.5",
    "js-nacl": "^1.2.2",
    "prompt-confirm": "^1.2.0",
    "ramda": "^0.25.0",
    "sanctuary": "^0.14.1",
    "truffle": "^4.1.5",
    "truffle-fast-compile": "^0.0.3",
    "web3": "^1.0.0-beta.33",
    "yargs": "^11.0.0"
  },
  "scripts": {
    "test": "cd svLight && truffle test",
    "test-watch": "cd svLight && reflex truffle test",
    "deploy": "node ./bin/deploy.js",
    "cli": "node -i ./bin/contractCli.js --name",
    "compile-sv-light": "for f in $(ls ./svLight/contracts | grep sol); do ./bin/compile.sh -d svLight -c $f; done",
    "compile-ens": "for f in $(ls ./ensSCs/contracts | grep sol); do ./bin/compile.sh -d ensSCs -c $f -o _distEns; done"
  }
}
